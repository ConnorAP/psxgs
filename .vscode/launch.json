	"version": "0.2.0",
	"configurations": [
		{
			"name": "Triangle",
			"type": "gdb",
			"request": "launch",
			"target": "${workspaceRoot}/samples/triangle/triangle.elf",
			"cwd": "${workspaceRoot}",
			"gdbpath": "mipsel-unknown-elf-gdb",
			"autorun": [
				"source ${workspaceRoot}/debug.gdb"
			],
			"valuesFormatting": "parseText",
			"preLaunchTask": "cleanBuildAll"
		},
	]
}